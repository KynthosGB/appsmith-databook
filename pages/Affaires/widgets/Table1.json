      "onClick": "{{\n  (() => {\n    const row = Table1.triggeredRow;\n    if (!row) {\n      showAlert(\"Aucune affaire sélectionnée\", \"warning\");\n      return;\n    }\n\n    CloturerAffaire.run()\n      .then(() => {\n        // Lance le workflow n8n qui déplace le dossier\n        return DeplacerDossierAffaire.run();\n      })\n      .then(() => {\n        showAlert(\"Affaire clôturée et dossier déplacé ✅\", \"success\");\n        return GetAffaires.run(); // rafraîchir la liste\n      })\n      .catch((e) => {\n        console.log(\"Erreur clôture / déplacement\", e);\n        showAlert(\"Erreur lors de la clôture ou du déplacement du dossier\", \"error\");\n      });\n  })()\n}}",
