      "onSave": "{{\n  (() => {\n    const base = TableEnceintesAppareil.triggeredRow;     // ligne d'origine\n    const updated = TableEnceintesAppareil.updatedRow || {}; // champs modifiés\n\n    if (!base) {\n      showAlert(\"Aucune ligne à mettre à jour\", \"warning\");\n      return;\n    }\n\n    // 1) Fusion : updated écrase base\n    const payload = { ...base, ...updated };\n\n    // 2) Normaliser : \"\" -> null\n    Object.keys(payload).forEach((key) => {\n      if (payload[key] === \"\") {\n        payload[key] = null;\n      }\n    });\n\n    // 3) Normalisation spécifique de la date\n    payload.date_epreuve = payload.date_epreuve || null;\n\n    // 4) Id en nombre (au cas où)\n    if (payload.id !== null && payload.id !== undefined) {\n      payload.id = Number(payload.id);\n    }\n\n    // 5) Appel de la requête\n    SaveLigneEncApp.run(payload)\n      .then(() => {\n        showAlert(\"Ligne mise à jour\", \"success\");\n        EnceintesAppareil.run(); // recharge la table\n      })\n      .catch((e) => {\n        showAlert(\"Échec de la mise à jour : \" + (e.message || e), \"error\");\n      });\n  })()\n}}\n",
