  "searchKey": "",
  "tableData": "{{\n(() => {\n  const rows = QryCCPUFull.data || [];\n\n  // --- statut global CCPU ---\n  const searchLabel = r => {\n    if (r.search_status === \"missing\" || r.search_status === \"need_manual_validation\") {\n      return \"⚠️ A choisir\";\n    } else if \n\t\t\t(r.search_status === \"path_ok\") return \"✔️ Choisi\";\n\t\t else if \n\t\t\t(r.search_status === \"no_code\") return \"❌ Code vide\";\n    return \"✔️ Trouvé\";\n  };\n\n  // --- statut copie (historique, peut rester même si tu ne copies plus) ---\n  const copyStatus = r => {\n    if (r.copied_at) return \"✔️ Copié\";\n    return \"⚠️ Non copié\";\n  };\n\n  return rows.map(r => ({\n    rep:  r.rep,\n    srsm: r.srsm,\n    code: r.code,\n\t\tep:  r.ep,\n    nuance: r.nuance,\n    norme: r.norme,\n\t\tdesignation: r.designation,\n\n    // nouveaux libellés\n    search_status_label: searchLabel(r),\n    copied_status_label: copyStatus(r),\n\n    // valeurs techniques\n    search_status : r.search_status,\n    src_path:  r.src_path,\n    dest_path: r.dest_path,\n    copied_at: r.copied_at,\n\n    // clé technique\n    pk: `${r.rep}|${r.srsm}|${r.code}`,\n  }))\n  .sort((a,b) => {\n    const repA  = String(a.rep  || \"\");\n    const repB  = String(b.rep  || \"\");\n    const srA   = Number(a.srsm || 0);\n    const srB   = Number(b.srsm || 0);\n    return repA.localeCompare(repB) || srA - srB;\n  });\n})()\n}}\n",
