      "buttonStyle": "rgb(3, 179, 101)",
      "cellBackground": "{{(() => { const tableData = Table1.processedTableData || []; return tableData.length > 0 ? tableData.map((currentRow, currentIndex) => (\n  (() => {\n    if (currentRow.date_reception) {\n      return \"#95e6b3\"; // vert : reçu\n    }\n\n    if (currentRow.date_commande) {\n      return \"#fde68a\"; // jaune : commandé\n    }\n\n    if (currentRow.numero_demande) {\n      return \"#fdba74\"; // orange : demande fournisseur\n    }\n\n    return \"white\"; // non traitée\n  })()\n)) : \n  (() => {\n    if (currentRow.date_reception) {\n      return \"#95e6b3\"; // vert : reçu\n    }\n\n    if (currentRow.date_commande) {\n      return \"#fde68a\"; // jaune : commandé\n    }\n\n    if (currentRow.numero_demande) {\n      return \"#fdba74\"; // orange : demande fournisseur\n    }\n\n    return \"white\"; // non traitée\n  })()\n })()}}",
      "columnType": "text",
      "computedValue": "{{(() => { const tableData = Table1.processedTableData || []; return tableData.length > 0 ? tableData.map((currentRow, currentIndex) => ( \n  (() => {\n    const dr = currentRow.date_reception;\n    if (dr) {\n      const dt = new Date(dr);\n      const day   = String(dt.getDate()).padStart(2, \"0\");\n      const month = String(dt.getMonth() + 1).padStart(2, \"0\");\n      const year  = dt.getFullYear();\n      return `Reçu le ${day}/${month}/${year}`;\n    }\n\n    const dc = currentRow.date_commande;\n    if (dc) {\n      const dt = new Date(dc);\n      const day   = String(dt.getDate()).padStart(2, \"0\");\n      const month = String(dt.getMonth() + 1).padStart(2, \"0\");\n      const year  = dt.getFullYear();\n      return `Commandé le ${day}/${month}/${year}`;\n    }\n\n    const nd = currentRow.numero_demande;\n    if (nd) {\n      return \"Demande fournisseur effectuée\";\n    }\n\n    return \"Non traitée par les achats\";\n  })()\n)) :  \n  (() => {\n    const dr = currentRow.date_reception;\n    if (dr) {\n      const dt = new Date(dr);\n      const day   = String(dt.getDate()).padStart(2, \"0\");\n      const month = String(dt.getMonth() + 1).padStart(2, \"0\");\n      const year  = dt.getFullYear();\n      return `Reçu le ${day}/${month}/${year}`;\n    }\n\n    const dc = currentRow.date_commande;\n    if (dc) {\n      const dt = new Date(dc);\n      const day   = String(dt.getDate()).padStart(2, \"0\");\n      const month = String(dt.getMonth() + 1).padStart(2, \"0\");\n      const year  = dt.getFullYear();\n      return `Commandé le ${day}/${month}/${year}`;\n    }\n\n    const nd = currentRow.numero_demande;\n    if (nd) {\n      return \"Demande fournisseur effectuée\";\n    }\n\n    return \"Non traitée par les achats\";\n  })()\n })()}}",
